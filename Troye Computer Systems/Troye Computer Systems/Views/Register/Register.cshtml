<body>
    <form id="form1" runat="server">
        <div>
            <div style="width:200px;margin-top: 5px; margin-left: 290px;  margin-bottom: 5px;">

            </div>
            <div>

                <div id="Register" class="tabcontent" style="width:900px;height: 2200px;margin-top: 90px; margin-left: 290px;  margin-bottom: 15px; background-color:#FFFFFF;">
                    <h1>Personal </h1>

                    <Label ID="txtName" runat="server" Text="Name">Name</Label> <input name="Name" id="Name" type="text" style="margin-top:2%; background-color:transparent; width:50%; height: 5%; border-top-color:transparent; border-left-color:transparent; border-left-width:thin; border-left-style:solid; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 29%; " />
                    <br />
                    <Label ID="txtSurname" runat="server" Text="Surname">Surname</Label><input name="Surname" type="text" style="margin-top:2%; background-color:transparent; width:50%; height: 5%; border-top-color:transparent; border-left-color:transparent; border-left-width:thin; border-left-style:solid; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 27%; " />
                    <br />
                    <Label ID="txtId" runat="server" Text="Id">Id</Label> <input name="Id" type="text" style="margin-top:2%; background-color:transparent; width:50%; height: 5%; border-top-color:transparent; border-left-color:transparent; border-left-width:thin; border-left-style:solid; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left:32%; " />
                    <br />
                    <br />

                    <h1>Conatact </h1>
                    <hr />
                    <Label ID="txtAddress" runat="server" Text="Address">Address</Label><input name="Adress" type="text" style="margin-top:2%; background-color:transparent; width:50%; height: 5%; border-top-color:transparent; border-left-color:transparent; border-left-width:thin; border-left-style:solid; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 27%; " />
                    <br />
                    <Label ID="txtCity" runat="server" Text="City">City</Label><input name="City" type="text" style="margin-top:2%; background-color:transparent; width:50%; height: 5%; border-top-color:transparent; border-left-color:transparent; border-left-width:thin; border-left-style:solid; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 29%; " />
                    <br />
                    <Label ID="txtProvince" runat="server" Text="Province">Province</Label><input name="Email" type="text" style="margin-top:2%; background-color:transparent; width:50%; height: 5%; border-top-color:transparent; border-left-color:transparent; border-left-width:thin; border-left-style:solid; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 26%; " />
                    <br />
                    <Label ID="txtPhone" runat="server" Text="Phone">Phone</Label><input name="Phone" type="text" placeholder="+2712345678" style="margin-top:2%; background-color:transparent; width:50%; height: 5%; border-top-color:transparent; border-left-color:transparent; border-left-width:thin; border-left-style:solid; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 28%; " />
                    <br />
                    <br />
                    <h1>Skills </h1>
                    <hr />

                    <Label ID="txtSkills" runat="server" Text="Skills">Skills</Label><input name="Name" type="text" placeholder="UX Designer" style="margin-top:2%; background-color:transparent; width:50%; height: 5%; border-top-color:transparent; border-left-color:transparent; border-left-width:thin; border-left-style:solid; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 26%; " />
                    <br />
                    <Label ID="txtYears" runat="server" Text="Years">Years</Label><input name="Password1" type="number" style="margin-top:2%;width :50%; height: 5%; background-color:transparent; border-top-color:transparent;border-left-color:transparent; border-left-style:solid; border-left-width:thin; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 26%; " />
                    <br />

                    <h1>Account </h1>
                    <hr />
                    <Label ID="txtEmail" runat="server" Text="Email">Email</Label><input name="Email  " type="text" style="margin-top:2%; background-color:transparent; width:50%; height: 5%; border-top-color:transparent; border-left-color:transparent; border-left-width:thin; border-left-style:solid; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 29%; " />
                    <br />
                    <Label ID="txtUsername" runat="server" Text="Username">Username</Label><input name="Name" type="text" placeholder="5559333" style="margin-top:2%; background-color:transparent; width:50%; height: 5%; border-top-color:transparent; border-left-color:transparent; border-left-width:thin; border-left-style:solid; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 26%; " />
                    <br />
                    <Label ID="txtPassword" runat="server" Text="Password">Password</Label><input name="Password1" type="password" placeholder="jUICYmilk5!" style="margin-top:2%;width :50%; height: 5%; background-color:transparent; border-top-color:transparent;border-left-color:transparent; border-left-style:solid; border-left-width:thin; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 26%; " />
                    <br />
                    <Label ID="txtConfirm" runat="server" Text="Confirm">Confirm</Label><input name="Confirm" type="password" style="margin-top:2%;width :50%; height: 5%; background-color:transparent; border-top-color:transparent;border-left-color:transparent; border-left-style:solid; border-left-width:thin; border-right-color:transparent;border-bottom-color:#000000; border-bottom-width:thin; margin-left: 26%; " />
                    <br />
                    <div>
                        <button runat="server" OnClick="btnRegister_Click" style="margin-top:10%; border-width: thin; margin-left: 39%; width :24%; height: 50px; background-color:#000000; border-style:solid; border-width:thin; font-family:'Century Gothic'; color:#ffffff; outline-color: #000000; border-radius:25px; margin-bottom:20px;" id="Button1">Register</button>
                    </div>
                </div>
            </div>
        </div>
    </form>


    <div>
        @RenderBody()
    </div>
</body>
</html>
